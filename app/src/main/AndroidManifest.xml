<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.selim.quotes">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/quotesicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/quotesicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quotes">
        <activity android:name="com.selim.quotes.Quotes"></activity>
        <activity android:name="com.selim.quotes.Admin" />
        <activity android:name="com.selim.quotes.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>